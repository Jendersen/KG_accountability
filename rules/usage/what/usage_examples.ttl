@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<> a kgi:TestQuery ;
    dcterms:title "Usage examples" ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:description "Provides some examples of the published data." ;
    kgi:query """
        PREFIX void: <http://rdfs.org/ns/void#>
        PREFIX schema: <http://schema.org/>
        PREFIX dcat: <http://www.w3.org/ns/dcat#>
        ASK {
            # Find the KG
            { ?kg a dcat:Dataset }
            UNION { ?kg a void:Dataset }
            ?kg ?endpointLink $rawEndpointUrl .
        
            {?kg void:exampleResource ?ex .}
            UNION {?kg schema:workExample ?ex .}
        }
""" .
