@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<> a kgi:TestQuery ;
    dcterms:title "Usage dataset entities" ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:description "How many entities, properties and classes are there in the dataset?" ;
    kgi:query """
        PREFIX void: <http://rdfs.org/ns/void#>
        PREFIX dcat: <http://www.w3.org/ns/dcat#>
        ASK {
            # Find the KG
            { ?kg a dcat:Dataset }
            UNION { ?kg a void:Dataset }
            ?kg ?endpointLink $rawEndpointUrl .
        
            ?kg void:entities ?nbEntities .
            ?kg void:properties ?nbProp .
            ?kg void:classes ?nbClasses .
        }
""" .
