@prefix kgi: <http://ns.inria.fr/kg/index#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<> a kgi:TestQuery ;
    dcterms:title "Usage start availability" ;
    dcterms:isReferencedBy <manifest.ttl> ;
    dcterms:description "Since when was the dataset available?" ;
    kgi:query """
        PREFIX void: <http://rdfs.org/ns/void#>
        PREFIX dcat: <http://www.w3.org/ns/dcat#>
        PREFIX prov: <http://www.w3.org/ns/prov#>
        PREFIX pav: <http://purl.org/pav/>
        PREFIX foaf: <http://xmlns.com/foaf/0.1/>
        PREFIX schema: <http://schema.org/>
        PREFIX dce: <http://purl.org/dc/elements/1.1/>
        PREFIX dcterms: <http://purl.org/dc/terms/>
        ASK {
            # Find the KG
            { ?kg a dcat:Dataset }
            UNION { ?kg a void:Dataset }
            ?kg ?endpointLink $rawEndpointUrl .
        
            {?kg dcterms:issued ?publicationDate .}
            UNION {?kg dcterms:available ?availableDate .}
            UNION {?kg schema:datePublished ?publicationDate .}
            UNION {?kg schema:sdDatePublished ?publicationDate .}
            UNION {?kg prov:wasGeneratedBy ?act .
                  ?act a prov:Publish .
                  {?act prov:startedAtTime ?pubStartDate .}
                  UNION {?act prov:endedAtTime ?pubEndDate .} }
        }
""" .
